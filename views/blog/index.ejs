<%- include ../layouts/header %>

	<% posts.forEach(function(post, index){ %>
	<div class="post">
		<p><h2><a href="#"><%= post.title %></a></h2></p>
		<p class="info">
			作者：<a href="#"><%= post.owner %></a>
			日期：<%= post.time.minute %>		
		</p>
		<p><%- post.content %></p>
	</div>
	<br /><br />
	<hr />
	<% }) %>


<%- include ../layouts/footer %>
